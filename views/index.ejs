<html>
<head>
    <title>Dashboard</title>
    <link rel="stylesheet" type="text/css" href="/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="css/dashboard.css">
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans" rel="stylesheet">
</head>
<body>
<nav class="navbar navbar-default navbar-fixed-top">
      <div id="nav-container" class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#"><i class="fa fa-tachometer" aria-hidden="true"></i> Dashboard</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">Components</a></li>
            <li><a href="#">Pipeline</a></li>
            
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>


<div id="main-content" class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12  col-md-10 col-md-offset-1">
            <h1 class="page-heading">Dashboard</h1> 
            <h4 class="sub-heading">Recent Updates</h4>

            <div class="row">
                <div class="col-sm-12">
                    <div class="table-responsive component status-fail">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-times" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="table-responsive component status-success">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-check" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="table-responsive component status-doubt">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-question" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="table-responsive component status-success">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-check" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="table-responsive component status-success">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-check" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="table-responsive component status-doubt">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class=""><i class="fa fa-question" aria-hidden="true"></i></th>
                                    <th class="">Component Name</th>
                                    <th class="">Pipeline Step</th>
                                    <th class="">More info</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>





            </div>


            <button class="btn btn-primary btn-lg">Show More</button>
            
        </div>
    </div>
</div>


   


    <footer class="footer">
      <div class="container">
        <p class="">Place footer content here.</p>
      </div>
