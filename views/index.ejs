<!DOCTYPE html>
<html ng-app="app">
<head>
    <title>Dashboard</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="css/dashboard.css">
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans" rel="stylesheet">
</head>
<body ng-controller="mainCtrl">
<nav class="navbar navbar-default navbar-fixed-top">
      <div id="nav-container" class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#"><i class="fa fa-tachometer" aria-hidden="true"></i> Dashboard</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/">Home</a></li>
            <li><a href="/versions">Versions</a></li>
            
            
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>


<div id="main-content" class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12  col-md-10 col-md-offset-1">
            <h1 class="page-heading">Dashboard</h1> 
            <h4 class="sub-heading">Recent Updates</h4>

            <div class="row" ng-repeat="version in versions">
                <div class="col-sm-12">
                    <div class="table-responsive component status-{{version.status}}">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class="">{{version.name}}</th>
                                    <th class="">{{version.step}}</th>
                                    <th class="">{{version.versionName}}</th>
                                </tr>
                            </thead>
                    
                        </table>
                    </div>
                </div>
            </div>


            <button class="btn btn-primary btn">Show More</button>
            
        </div>
    </div>
</div>


   


    <footer class="footer">
      <div class="container">
        <p class="text-muted">Place footer content here.</p>
      </div>
    </footer>


<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous"></script>
<script type="text/javascript" src="js/bootstrap.min.js"></script>
<script type="text/javascript" src="js/angular.min.js"></script>
<script type="text/javascript" src="js/app.js"></script>
</body>
</html>