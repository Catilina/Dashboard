<!DOCTYPE html>
<html ng-app="app">
<head>
    <title>Dashboard</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="css/dashboard.css">
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans" rel="stylesheet">
</head>
<body ng-controller="mainCtrl">
<nav class="navbar navbar-default navbar-fixed-top">
      <div id="nav-container" class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/"><i class="fa fa-tachometer" aria-hidden="true"></i> Dashboard</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/">Home</a></li>
            <li><a href="/components">Components</a></li>
            <!-- <li><a href="/pipeline">Pipeline</a></li> --> 
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>


<div id="main-content" class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="page-heading">Dashboard</h1> 
            <div class=" row">
                <div class="col-sm-6">
                    <div class="panel panel-default">
                        <a class="panel-custom" data-toggle="collapse" href="#collapse">
                            <div class="panel-heading update-panel">
                                <h4>
                                    Component Pipeline Step Breakdown
                                </h4>
                            </div>
                        </a>

                        <div id="donutchart" style="width: auto; max-height: auto;"></div>
                    </div>
                </div>

                <div class="col-sm-6">
                    <div class="panel panel-default">
                        <a class="panel-custom" data-toggle="collapse" href="#collapse">
                            <div class="panel-heading update-panel">
                                <h4>
                                    Component Status Breakdown
                                </h4>
                            </div>
                        </a>

                        <div id="donutchart2" style="width: auto; max-height: auto;"></div>
                    </div>
                </div>
            </div>
            


                <div class="panel panel-default">
                    <a class="panel-custom" data-toggle="collapse" href="#collapse">
                        <div class="panel-heading update-panel">
                            <h4>
                                Recent Updates 
                            </h4>
                        </div>
                    </a>
                                        
                    <div class="table-responsive"> 
                                                
                    <table id="attendTable" class="table table-bordered ">
                        <thead>
                            <tr >
                                <th>Component</th>
                                <th>Version</th>
                                <th>Step</th>
                                <th>Status</th>                                                        
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="version in versions" class="">
                                <td>{{version.name}}</td>
                                <td>{{version.versionName}}</td>
                                <td>{{version.step}}</td>
                                <td>{{version.statusName}}</td>
                            </tr>
                        </tbody>
                    </table>                                


                    </div>
                                            
                                        
             </div>

                 
        </div>
    </div>
</div>


   


    <footer class="footer">
      <div class="container">
        <p class="text-muted">Place footer content here.</p>
      </div>
    </footer>


<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous"></script>
<script type="text/javascript" src="js/bootstrap.min.js"></script>
<script type="text/javascript" src="js/angular.min.js"></script>
<script type="text/javascript" src="js/ui-bootstrap.min.js"></script>
<script type="text/javascript" src="js/app.js"></script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
</body>
</html>